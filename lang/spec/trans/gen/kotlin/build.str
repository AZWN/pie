module gen/kotlin/build

imports
  
  libspoofax/stratego/debug
  
  gen/kotlin/core

rules

  generate-kotlin-file:
    (ast, _, _, file, _) -> (filename, result)
    with
      filename := <guarantee-extension(|"kt")> file
    ; mod      := <base-filename;remove-extension> file
    ; result   := <p2k(|mod)> ast
