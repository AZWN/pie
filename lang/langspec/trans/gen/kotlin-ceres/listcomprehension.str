module gen/kotlin-ceres/listcomprehension

imports

  signatures/-
  signatures/lex/-
  signatures/impl/-
  
  gen/kotlin-ceres/exp
  gen/kotlin-ceres/binder

rules

  p2c-listcomprehension:
    ListComprehension(mapExp, binder, listExp) -> $[[listExpStr].map {[binderStr] -> [mapExpStr]}]
    with
      mapExpStr  := <try-p2c-exp> mapExp
    ; binderStr  := <p2c-binder> binder
    ; listExpStr := <try-p2c-exp> listExp
