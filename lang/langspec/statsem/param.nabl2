module statsem/param

rules

  [[ Params(params) ^ (s) : tys ]] :=
    Map1T [[ params ^ (s) : tys ]].

  [[ Param(ty) ^ (s) : ty_bound ]] :=
    [[ ty ^ (s) : ty_bound ]].
    
  [[ NParam(id_var, ty) ^ (s) : ty_bound ]] :=
     Var{id_var} <- s,
     Var{id_var}.kind := InputVarKind() !,
     [[ ty ^ (s) : ty_bound ]],
     Var{id_var} : ty_bound !.
